./gradlew clean sonarqube -Dsonar.verbose=true --stacktrace --debug



 -Dsonar.host.url=https://sonarqube-devhub-tools.pathfinder.gov.bc.ca -Dsonar.verbose=true --stacktrace --debug \
-Dsonar.projectName=signing-web -Dsonar.branch=master -Dsonar.projectKey=org.sonarqube:signing-web \
-Dsonar.sources=src/ \
-Dsonar.tests=tests/ \
-Dsonar.testExecutionReportPaths=src/tests-report-fake.xml



-Dsonar.tests=src/ -Dsonar.testExecutionReportPaths=src/tests-report-fake.xml



-Dsonar.tests=src/ -Dsonar.testExecutionReportPaths=src/tests-report-fake.xml



-Dsonar.testExecutionReportPaths=tests-report-fake.xml


./gradlew sonarqube -Dsonar.host.url=https://sonarqube-devhub-tools.pathfinder.gov.bc.ca 
-Dsonar.verbose=true --stacktrace 
-Dsonar.projectName=signing-web -Dsonar.branch=master -Dsonar.projectKey=org.sonarqube:signing-web

 -Dsonar.sources=.. -Dsonar.tests=../src/ 
